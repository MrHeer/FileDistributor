{
  "name": "FileDistributorWeb",
  "version": "1.0.0",
  "description": "File Distribute Platform",
  "main": "index.js",
  "scripts": {
    "start": "umi dev",
    "dev": "umi dev",
    "dev:no-mock": "MOCK=none umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "keywords": [
    "file",
    "distribute",
    "react",
    "antd",
    "umi"
  ],
  "author": "MrHeer",
  "license": "ISC",
  "devDependencies": {
    "@types/react-document-title": "^2.0.4",
    "@types/react-infinite-scroller": "^1.2.1",
    "@umijs/preset-react": "1.6.3",
    "@umijs/test": "^3.2.5",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "umi": "^3.2.5",
    "yorkie": "^2.0.0"
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
    "@ant-design/pro-layout": "^5.0.12",
    "react": "^16.12.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.12.0",
    "react-infinite-scroller": "^1.2.4"
  }
}
